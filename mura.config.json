{
	"global":{
		"rendererProperties":{
			"templateArray":["default"],
			"collectionLayoutArray":["CollectionLayout","LatestArticleCarousel", "RecentArticles"],
			"hashurls":false,
			"primaryContentTypes":"Page,Link,File",
			"editableAttributesArray":[],
			"imageAttributesArray":[]
		},
		"modules":{
			"Example":{
				"name":"My Module",
				"contenttypes":"*",
				"omitcontenttypes":"",
				"configurator":[
					{"type":"text","name":"myvar","label":"My Label"}
				]
			},
			
			"LatestArticle":{
				"name":"Latest Article",
				"contenttypes":"Page/Article",
				"configurator":[
					{"type":"image","name":"articleSourceImage","label":"Article source image"},
					{"type":"text","name":"articleAuthors","label":"Article Author(s)"},
					{"type":"text","name":"articleAuthorAdditionalInfo","label":"Article Author additional info (Company & Title)"},					
					{"type":"text","name":"businessSourceHeadline","label":"Business Source Headline"},
					{"type":"text","name":"articleTitle","label":"Article Title"},
					{"type":"text","name":"articleBodyCopyPreview","label":"Article Body Copy Preview"},
					{"type":"text","name":"downloadWhitePaperCTA","label":"Download White Paper CTA"},
					{"type":"text","name":"seeAllInsightsCTA","label":"See all Insights CTA"}
				]
			},
			"CollectionLayout":{
				"name":"CollectionLayout",
				"contenttypes":"",
				"configurator":[
					{"type":"hidden","name":"fields","value":"title,filename"}
				]
			},
			"LatestArticleCarousel":{
				"name":"Latest Article Carousel",
				"contenttypes":"*",
				"configurator":[
					{"type":"text","name":"businessSource","label":"Business Source"},
				]
			},
			"ContentCarousel": {
				"name": "Recent Article Carousel",
				"contenttypes":"*",
				"configurator":[
					{"type":"text","name":"businessSource","label":"Business Source"},
				]
			},
			"MarketStats":{
				"name":"Market Statistics",
				"contenttypes":"*",
				"configurator":[
					{"type":"text","name":"sectionTitle","label":"Section Title"},
					{"type":"textarea","name":"sectionDescription","label":"Section Description"},
					{"type":"text","name":"businessSource","label":"Business Source"},
					{"type":"text","name":"moduleName","label":"Module Name"}
				]
			},
			"FeaturedVideo":{
				"name":"Featured Video",
				"contenttypes":"*",
				"configurator":[
					{"type":"image","name":"logoImage","label":"Logo Image"},
					{"type":"image","name":"posterImage","label":"Poster Image Overwrite"},
					{"type":"text","name":"youtubeId","label":"Youtube ID"},
					{"type":"text","name":"titleOverwrite","label":"Title Overwrite"},
					{"type":"text","name":"businessSource","label":"Business Source"},
					{"type":"text","name":"moduleName","label":"Module Name"},
					{"type":"text","name":"cta","label":"CTA Link"},
					{"type":"text","name":"ctaLabel","label":"CTA Text"}
				]
			},
			"TitleTextModule":{
				"name":"Title Text Module",
				"contenttypes":"*",
				"configurator":[
					{"type":"text","name":"pageTitle","label":"Page Title"},
					{"type":"text","name":"headline","label":"Headline"},
					{"type":"textarea","name":"leftColumn","label":"Left Column"},
					{"type":"textarea","name":"rightColumn","label":"Right Column"},
				]
			},
			"LinkList":{
				"name":"Link List",
				"contenttypes":"*",
				"configurator":[
					{"type":"textarea","name":"description","label":"Description"}
				]
			},
			"TwoImage":{
				"name":"Two Image Module",
				"contenttypes":"*",
				"configurator":[
					{"type":"text","name":"copy","label":"Copy"},
					{"type":"image","name":"image1","label":"First Image"},
					{"type":"image","name":"image2","label":"Second Image"},
					{"type":"select", "name": "layout", "label": "Layout", "options": ["Title Left", "Description Left", "Title Above"]},
					{"type":"select", "name": "topSpacing", "label": "Top Spacing", "options": ["Large", "Small"]},
					{"type":"select", "name": "bottomSpacing", "label": "Bottom Spacing", "options": ["Large", "Small"]}

				]
			},
		},
	"sites":{
		"default":{
			"rendererProperties":{

			},
			"modules":{

			}
		}
	}
	}
}
